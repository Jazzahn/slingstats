%table.table.table-striped.offset2.span8
  %thead
    %tr
      %th Player Name
      %th Matches Played
      %th Matches Won
      %th Win Percentage
  %tbody
    -@players.each do |player|
      %tr
        %td=link_to player.name, player
        %td=player.matches
        %td=player.matcheswon
        %td #{player.winning_ratio}%
